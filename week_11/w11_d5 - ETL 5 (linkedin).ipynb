    "\n",
    "\n",
    "results=[]\n",
    "\n",
    "for c in countries:\n",
    "    for k in keywords:\n",
    "        for ex in range(1, 4):\n",
    "            try:\n",
    "                results+=Linkedin().search(k, 5, c, 1e6, ex)\n",
    "            except:\n",
    "                continue"
